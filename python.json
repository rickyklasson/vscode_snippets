	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	},
	"Argparse template": {
		"prefix": "argtemp",
		"body": [
			"#! /usr/bin/python3",
			"import argparse",
			"",
			"def main(args):",
			"\tprint(args)",
			"",
			"if __name__ == '__main__':",
			"\tparser = argparse.ArgumentParser(description=\"<Script description>\")",
			"",
			"\tparser.add_argument(\"-s\", \"--single\", type=str, help=\"\")",
			"\tparser.add_argument(\"-m\", \"--multi\", type=str, nargs=2, help=\"\")",
			"\tparser.add_argument(\"-l\", \"--list\", type=int, nargs='+', help=\"\", default=[])",
			"\tparser.add_argument(\"-b\", \"--boolean\", help=\"\", action='store_true')",
			"\t",
			"\targs = parser.parse_args()",
			"\tmain(args)"
		],
		"description": "Create argparse script template"
	}
}